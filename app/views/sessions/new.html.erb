<div class="waviy">
  <span class="head1", style="color:#990011FF">Login</span>
</div>
<div class="midd">
  <%= form_for(:session, :html => {class: "form-horizontal", role: "form"}, url: login_path) do |f| %>
    <div class="from-group">
      <div class="control-label col-sm-3">
        <span class="head1"><%= f.label :email%></span>
      </div>
      <div class="col-sm-10">
        <%= f.email_field :email, class: "form-control", placeholder: "Enter Email", autofocus: true %>
      </div>
    </div>
    <br>
    <div class="from-group">
      <div class="control-label col-sm-3">
        <span class="head1"><%= f.label :password%></span>
      </div>
      <div class="col-sm-10">
        <%= f.password_field :password ,class: "form-control", autocomplete: "off" %>
      </div>
    </div>
    <br>
    <div class ="abc">
      <div class="form-group">
        <div class="col-sm-10">
          <%= f.submit "Login" , class: 'effect effect-5' %>
        </div>
      </div>
    </div>
  </div>
<% end %>
</div>
